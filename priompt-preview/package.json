{
  "name": "@anysphere/priompt-preview",
  "license": "MIT",
  "version": "0.1.0",
  "description": "An interactive preview of priompt prompts.",
  "repository": {
    "type": "git",
    "url": "https://github.com/anysphere/priompt"
  },
  "homepage": "https://github.com/anysphere/priompt",
  "author": "Arvid Lunnemark",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "publish-to-npm": "pnpm build && pnpm publish --access=public",
    "build-watch": "nodemon --watch 'src/**/*' --ext '*' --exec 'pnpm build'",
    "fast-build-watch": "watchexec -w src pnpm build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-slot": "^1.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cmdk": "^0.2.0",
    "lucide-react": "^0.263.1",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.6",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@anysphere/priompt": "workspace:*",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "axios": "^0.26.1",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "nodemon": "^2.0.22",
    "postcss": "^8.4.27",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "use-debounce": "^9.0.4",
    "vite": "^4.3.2"
  },
  "bin": {
    "serve": "./scripts/serve.cjs"
  }
}
