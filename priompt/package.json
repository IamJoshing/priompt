{
  "name": "@anysphere/priompt",
  "license": "MIT",
  "version": "0.1.0",
  "description": "A JSX-based prompt design library.",
  "keywords": [
    "prompting",
    "prompt design",
    "prompt engineering"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/anysphere/priompt"
  },
  "homepage": "https://github.com/anysphere/priompt",
  "author": "Arvid Lunnemark",
  "engines": {
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm",
    "pnpm": ">= 8.3.0 < 9",
    "node": ">= 18.15.0 < 19"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "watch": "tsx watch --clear-screen=false src",
    "publish-to-npm": "pnpm build && pnpm publish --access=public",
    "build": "tsc && cp src/types.d.ts dist/types.d.ts",
    "build-watch": "nodemon --watch 'src/**/*' --ext '*' --exec 'pnpm build'",
    "lint": "tsc --noEmit && eslint .",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.5",
    "@types/json-schema": "^7.0.12",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitest/coverage-c8": "^0.30.1",
    "esbuild": "^0.17.17",
    "eslint": "^8.38.0",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.0",
    "tiny-glob": "^0.2.9",
    "tsx": "^3.12.6",
    "typescript": "^5.0.4",
    "vitest": "^0.30.1"
  },
  "dependencies": {
    "js-yaml": "https://github.com/anysphere/js-yaml.git#4761daebc257cf86e64bb775ba00696f30d7ff22",
    "openai": "^3.3.0",
    "tiktoken-node": "^0.0.6",
    "zod": "^3.21.4",
    "zod-to-json-schema": "^3.21.3"
  }
}
