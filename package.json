{
  "name": "priompt",
  "license": "MIT",
  "engines": {
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm",
    "pnpm": ">= 8.3.0 < 9",
    "node": ">= 18.15.0 < 19"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "watch": "tsx watch --clear-screen=false src",
    "build": "tsc && cp src/types.d.ts dist/types.d.ts",
    "build-watch": "nodemon --watch 'src/**/*' --ext '*' --exec 'pnpm build'",
    "lint": "tsc --noEmit && eslint .",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitest/coverage-c8": "^0.30.1",
    "esbuild": "^0.17.17",
    "eslint": "^8.38.0",
    "eslint-plugin-sonarjs": "^0.19.0",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.0",
    "tiny-glob": "^0.2.9",
    "tsx": "^3.12.6",
    "typescript": "^5.0.4",
    "vitest": "^0.30.1"
  },
  "dependencies": {
    "js-yaml": "https://github.com/anysphere/js-yaml.git#4761daebc257cf86e64bb775ba00696f30d7ff22",
    "openai": "^3.2.1",
    "tiktoken-node": "^0.0.6"
  }
}
